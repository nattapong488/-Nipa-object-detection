{
  "name": "@nipacloud/nvision",
  "version": "0.1.7",
  "description": "Nvision Client library",
  "main": "dist/nvision.js",
  "typings": "dist/nvision.d.ts",
  "browser": {
    "dist/nvision.js": "dist/browser/nvision.js"
  },
  
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf ./dist; yarn protogen; mkdir ./dist ./dist/proto; cp -r ./src/proto/generated ./dist/proto; yarn webpack;",
    "link": "yarn build && cd dist && yarn link",
    "build-local": "NODE_ENV=none yarn build",
    "build-dev": "NODE_ENV=development yarn build",
    "build-prod": "NODE_ENV=production yarn build",
    "build-tsc": "rm -rf ./dist; yarn protogen; mkdir ./dist ./dist/proto; cp -r ./src/proto/generated ./dist/proto; tsc",
    "protogen": "cd src/proto; mkdir -p ./generated; protoc --plugin=\"protoc-gen-ts=../../node_modules/.bin/protoc-gen-ts\" --js_out=\"import_style=commonjs,binary:./generated\" --ts_out=\"./generated\" *.proto"
  },
  "keywords": [
    "typescript",
    "library",
    "nipacloud",
    "nvision",
    "ai"
  ],
  "author": "Chayapol Limanon <chayapol@nipa.cloud>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@types/debug": "^4.1.5",
    "@types/google-protobuf": "^3.7.2",
    "@types/node": "^13.1.6",
    "@types/ws": "^6.0.3",
    "babel": "^6.23.0",
    "babel-loader": "^8.0.6",
    "dts-bundle": "^0.7.3",
    "ts-loader": "^6.1.0",
    "ts-node": "^8.3.0",
    "ts-protoc-gen": "^0.11.0",
    "typescript": "^3.7.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dts-bundle": "^0.0.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bufferutil": "^4.0.1",
    "google-protobuf": "^3.10.0-rc.1",
    "is-buffer": "^2.0.4",
    "lodash": "^4.17.15",
    "utf-8-validate": "^5.0.2",
    "ws": "^7.2.1"
  }
}
